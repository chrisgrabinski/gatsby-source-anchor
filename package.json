{
  "name": "gatsby-source-anchor",
  "version": "0.5.1",
  "description": "Source plugin to fetch podcast episodes from Anchor into Gatsby",
  "author": "Henrique Cavalieri <@hcavalieri>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hcavalieri/gatsby-source-anchor/issues"
  },
  "homepage": "https://github.com/hcavalieri/gatsby-source-anchor",
  "repository": {
    "type": "git",
    "url": "https://github.com/hcavalieri/gatsby-source-anchor"
  },
  "main": "gatsby-node.js",
  "scripts": {
    "precommit": "tsc",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source",
    "anchor",
    "podcast"
  ],
  "dependencies": {
    "rss-parser": "^3.5.3"
  },
  "devDependencies": {
    "@types/node": "^10.12.2",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.6"
  },
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
